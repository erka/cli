{
  "$schema": "../../flag_manifest.json",
  "flags": {
    "booleanFlag": {
      "flagType": "boolean",
      "codeDefault": true
    },
    "stringFlag": {
      "flagType": "string",
      "codeDefault": "default"
    },
    "integerFlag": {
      "flagType": "integer",
      "codeDefault": 50
    },
    "floatFlag": {
      "flagType": "float",
      "codeDefault": 0.15
    },
    "objectFlag": {
      "flagType": "object",
      "codeDefault": {
        "primaryColor": "#007bff",
        "secondaryColor": "#6c757d"
      }
    }
  }
}